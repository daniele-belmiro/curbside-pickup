    "Templates": {
        "email": {
            "To": "{{curbsidePickup.toEmail}}",
            "CC": "marcelo@vtex.com",
            "BCC": null,
            "Subject": "New Pickup Order #{{order.orderId}}",
            "Message": "<!-- begin sum of taxes test -->\r\n{{eval \"Handlebars.registerHelper('sumTaxes', function(items) {\r\n    var x = 0;\r\n    for(var i in items) {\r\n      if (items[i].id.toLowerCase().indexOf('tax') !== -1) {\r\n          x += items[i].value;\r\n      }\r\n    }\r\n    return x;\r\n  });\"}}\r\n  \r\n{{eval \"Handlebars.registerHelper('sumTaxesSku', function(items) {\r\n    var x = 0;\r\n    for(var i in items) {\r\n      if (items[i].name.indexOf('TAXHUB@') !== -1) {\r\n          x += items[i].value;\r\n      }\r\n    }\r\n    return x;\r\n  });\"}}\r\n  \r\n{{eval \"Handlebars.registerHelper('splitMail', function(title) {\r\n  var t = title.split('-');\r\n  return t[0];\r\n  });\"}}\r\n  \r\n{{eval \"Handlebars.registerHelper('index_of', function(context,ndx) {\r\n  return context[ndx];\r\n  });\"}}\r\n\r\n\r\n                                \r\n<!-- end sum of taxes test -->\r\n<!DOCTYPE html\r\n    PUBLIC \"-//W3C//DTD XHTML 1.0 Transitional //EN\" \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd\">\r\n<html xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:o=\"urn:schemas-microsoft-com:office:office\"\r\n    xmlns:v=\"urn:schemas-microsoft-com:vml\"\r\n    style=\"-webkit-text-size-adjust: 100%; -ms-text-size-adjust: 100%; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale; box-sizing: border-box; width: 100%; height: 100%; margin: 0; padding: 0; background: #f1f1f1 !important;\">\r\n\r\n<head>\r\n    <meta http-equiv=\"Content-Type\" content=\"text/html; charset=UTF-8\" />\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\" />\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\r\n    <title>{{_accountInfo.TradingName}}</title>\r\n    <!--[if gte mso 9]>\r\n  <xml>\r\n    <o:OfficeDocumentSettings>\r\n    <o:AllowPNG/>\r\n    <o:PixelsPerInch>96</o:PixelsPerInch>\r\n    </o:OfficeDocumentSettings>\r\n  </xml>\r\n  <![endif]-->\r\n    <style>\r\n        a[x-apple-data-detectors] {\r\n            color: inherit !important;\r\n            text-decoration: none !important;\r\n            font-size: inherit !important;\r\n            font-family: inherit !important;\r\n            font-weight: inherit !important;\r\n            line-height: inherit !important;\r\n        }\r\n    </style>\r\n    <style>\r\n        @media (max-width: 600px) {\r\n\r\n            img {\r\n                max-width: 100% !important;\r\n                height: auto !important;\r\n            }\r\n        }\r\n    </style>\r\n    <style>\r\n        @media screen and (min-width: 30em) {\r\n\r\n            .ph4-ns {\r\n                padding-left: 2rem !important;\r\n                padding-right: 2rem !important;\r\n            }\r\n\r\n            .mv4-ns {\r\n                margin-top: 2rem !important;\r\n                margin-bottom: 2rem !important;\r\n            }\r\n        }\r\n    </style>\r\n</head>\r\n\r\n<body\r\n    style=\"-webkit-text-size-adjust: 100%; -ms-text-size-adjust: 100%; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale; box-sizing: border-box; width: 100%; height: 100%; margin: 0; padding: 0; background: #f1f1f1 !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\">\r\n    <table width=\"100%\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\"\r\n        style=\"box-sizing: border-box; margin: 0; padding: 0; background: #f1f1f1; border-collapse: collapse; border-spacing: 0; mso-table-lspace: 0pt; mso-table-rspace: 0pt; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif; width: 100%; height: 100%; line-height: 100% !important; color: #000;\">\r\n        <tr style=\"box-sizing: border-box !important;\">\r\n            <td align=\"left\" valign=\"top\"\r\n                style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\">\r\n                <table class=\"mv4-ns\" width=\"100%\" align=\"center\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\"\r\n                    style=\"box-sizing: border-box; max-width: 40rem; width: 100%; background-color: #fff; border-collapse: collapse; border-spacing: 0; mso-table-lspace: 0pt; mso-table-rspace: 0pt !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif; color: #000; \"\r\n                    bgcolor=\"#fff\">\r\n\r\n                    <tr style=\"box-sizing: border-box !important;\">\r\n                        <td class=\"ph4-ns\" style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; border-bottom-style: solid; border-bottom-width: 1px; border-color: #eee; width: 100%; padding-bottom: 2rem; text-align:left !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\">\r\n                            <h1 style=\"margin: 0; font-size: 22px; line-height: 58px; box-sizing: border-box !important; color: #000\">\r\n                                Order #{{order.orderId}}\r\n                            </h1>\r\n                            <div style=\"box-sizing: border-box; color: #777; margin-top: .5rem !important;\">\r\n                                Order Date <span style=\"font-weight: 700 !important;\">{{formatUSDate order.creationDate}}</span><br/>\r\n                                Name: <span style=\"font-weight: 700 !important;\">{{order.clientProfileData.firstName}} {{order.clientProfileData.lastName}}</span><br/>\r\n                                Email: <span style=\"font-weight: 700 !important;\">{{splitMail order.clientProfileData.email}}</span><br/>\r\n                                Phone: <span style=\"font-weight: 700 !important;\">{{order.clientProfileData.phone}}</span>\r\n                            </div>\r\n                        </td>\r\n                    </tr>\r\n                    \r\n                    <tr style=\"box-sizing: border-box !important;\">\r\n                        <td class=\"ph4-ns\" style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; border-bottom-style: solid; border-bottom-width: 1px; border-color: #eee; width: 100%; padding-bottom: 2rem; text-align:left !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\">\r\n                            <h3 style=\"margin: 0; font-size: 16px; line-height: 36px; text-transform: uppercase; letter-spacing: 1.2pt; font-weight: 300; box-sizing: border-box; margin-top: 0 !important;\">\r\n                                Payment\r\n                            </h3>\r\n                            <table width=\"100%\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\">\r\n        \t\t\t\t\t\t{{#each order.totals}}\r\n    \t\t\t\t\t\t\t\t{{#if value}}\r\n    \t\t\t\t\t\t\t\t{{#hasSubStr id \"Tax\"}}\r\n    \t\t\t\t\t\t\t\t{{else}}\r\n\t\t\t\t\t\t\t\t\t\t\r\n                                    <tr>\r\n                                       <td style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; padding-top: 0px; padding-bottom: 0px; font-family: Arial, sans-serif;\" align=\"left\">\r\n    \t\t\t\t\t\t\t\t\t\t{{#eq id \"Items\"}}Subtotal ({{../subjectItemAttachment.totalItems}} items){{/eq}}\r\n\t\t\t\t\t\t\t\t\t\t\t{{#eq id \"Shipping\"}}Shipping{{/eq}}\r\n\t\t\t\t\t\t\t\t\t\t\t{{#eq id \"Discounts\"}}Discounts{{/eq}}\r\n\t\t\t\t\t\t\t\t\t\t\t{{#eq id \"Tax\"}}Tax{{/eq}}\r\n\t\t\t\t\t\t\t\t\t\t\t{{#eq id \"Change\"}}{{name}}{{/eq}}\r\n\t\t\t\t\t\t\t\t\t\t  </td>\r\n                                        <td style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; padding-top: 0px; padding-bottom: 0px; text-align: right !important; font-family: Arial, sans-serif;\" align=\"right\">\r\n                                        $ {{formatUSDCurrency value}}\r\n                                        </td>\r\n                                    </tr>\r\n\t\t\t\t\t\t\t\t\t{{/hasSubStr}}\r\n\t\t\t\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t\t\t\t{{/each}}\r\n\t\t\t\t\t\t\t\t\t{{#if (sumTaxes order.totals)}}\r\n                                        <tr>\r\n                                            <td style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; padding-top: 0px; padding-bottom: 0px; font-family: Arial, sans-serif;\" align=\"left\">\r\n                                            Tax\r\n                                            </td>\r\n                                            <td style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; padding-top: 0px; padding-bottom: 0px; text-align: right !important; font-family: Arial, sans-serif;\" align=\"right\">\r\n                                            $ {{formatUSDCurrency (sumTaxes order.totals)}}\r\n                                            </td>\r\n                                        </tr>\r\n\t\t\t\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t\t\t\t\t{{#if order.value}}\r\n                                        <tr>\r\n                                            <td style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; font-weight: 500; padding-top: 15px; padding-bottom: .25rem !important; font-family: Arial, sans-serif; border-top-style: solid; border-top-width: 1px; border-color: #CFCFCF; font-weight: bold;\" align=\"left\">Order Total</td>\r\n                                            <td style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; font-weight: 500; padding-top: 15px; padding-bottom: .25rem; text-align: right !important; font-family: Arial, sans-serif; border-top-style: solid; border-top-width: 1px; border-color: #CFCFCF; font-weight: bold;\" align=\"right\">$ {{formatUSDCurrency order.value}}</td>\r\n                                        </tr>\r\n\t\t\t\t\t\t\t\t\t{{/if}}\r\n                                        \r\n                                    {{#each order.paymentData.transactions}}\r\n                                        {{#compare payments.length \"==\" 1}}\r\n                                            {{#each payments}}\r\n                                                <tr>\r\n                                                    <td style=\"font-size: 12px; color: #777; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; padding-top: 0px; padding-bottom: 0px; font-family: Arial, sans-serif;\" align=\"left\">\r\n                                                    {{paymentSystemName}}\r\n                                                    {{#if lastDigits}} last digits {{lastDigits}} {{/if}}\r\n                                                    </td>\r\n                                                    <td style=\"font-size: 12px; color: #777; line-height: 20px; box-sizing: border-box; border-collapse: collapse; padding-top: 0px; padding-bottom: 0px; text-align: right !important; font-family: Arial, sans-serif;\" align=\"right\">\r\n                                                    $ {{formatUSDCurrency value}}\r\n                                                    </td>\r\n                                                </tr>\r\n                                            {{/each}}\r\n                                        {{/compare}}\r\n                                    {{/each}}\r\n                                </table>\r\n                        </td>\r\n                    </tr>\r\n                    <tr style=\"box-sizing: border-box !important;\">\r\n                        <td class=\"ph4-ns\" style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; border-bottom-style: solid; border-bottom-width: 1px; border-color: #eee; width: 100%; padding-bottom: 2rem; text-align:left !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\">\r\n                            <h3 style=\"margin: 0; font-size: 16px; line-height: 30px; text-transform: uppercase; letter-spacing: 1.2pt; font-weight: 300; box-sizing: border-box; margin-top: 10px !important;\">\r\n                                Summary of your order\r\n                            </h3>\r\n                            <!-- PickUp -->\r\n                            Pickup at Store: {{slas.0.pickupStoreInfo.friendlyName}}<br/>\r\n                            \r\n                            {{#each order.shippingData.logisticsInfo}}\r\n                                {{#eq selectedDeliveryChannel \"pickup-in-point\"}}\r\n                                    </br>\r\n                                    {{#each ../order.items}}\r\n                                        {{#eq ../itemIndex @index}}\r\n                                         <table>\r\n                                            <tr style=\"box-sizing: border-box !important;\">\r\n                                            <td style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; width: 4rem; padding-right: .5rem; padding-top: .5rem; padding-bottom: .5rem; vertical-align: top; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\" align=\"left\" valign=\"top\">\r\n                                                <img alt=\"\" src=\"{{imageUrl}}\" style=\"vertical-align: top; outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; max-width: 100% !important;\" />\r\n                                            </td>\r\n                                            <td style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; padding-top: .5rem; padding-bottom: .5rem; vertical-align: top !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\"\r\n                                                align=\"left\" valign=\"top\">\r\n                                                <div class=\"mv1-ns\" style=\"box-sizing: border-box; line-height: 1.25; !important;\">\r\n                                                    {{name}}\r\n                                                </div>\r\n                                                <div style=\"box-sizing: border-box; font-size: 14px; !important;\">\r\n                                                <b>{{quantity}}</b> quantity\r\n                                                </div>\r\n                                                <div style=\"box-sizing: border-box; font-size: 14px; !important;\">\r\n                                                ALU Code: {{refId}}\r\n                                                </div>\r\n                                                <div style=\"box-sizing: border-box; !important;\">\r\n                                                    \r\n                                                    {{#if sellingPrice}}\r\n                                                    Price: <b>$ {{formatUSDCurrency sellingPrice}}</b>\r\n                                                    {{else}}\r\n                                                    Free\r\n                                                    {{/if}}\r\n                                                </div>\r\n                                                <div style=\"box-sizing: border-box; !important;\">\r\n                                                    Tax: $ {{formatUSDCurrency (sumTaxesSku priceTags)}}\r\n                                                </div>\r\n                                            </td>\r\n                                            </tr>\r\n                                        </table>\r\n                                       {{/eq}}     \r\n                                    {{/each}}\r\n                                {{/eq}}\r\n                            {{/each}}\r\n                        </td>\r\n                    </tr>    \r\n                    <!-- Other delivery types --> \r\n                    <tr style=\"box-sizing: border-box !important;\">\r\n                        <td class=\"ph4-ns\" style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; border-top-style: solid; border-top-width: 1px; border-color: #eee; padding-top: 1.5rem; padding-bottom: 1rem !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\" align=\"left\">\r\n                            Others<br/>\r\n                            {{#each order.shippingData.logisticsInfo}}\r\n                            {{#eq selectedDeliveryChannel \"pickup-in-point\"}}\r\n                            {{else}}\r\n                                    {{#each ../order.items}}\r\n                                        {{#eq ../itemIndex @index}}\r\n                                         <table>\r\n                                            <tr style=\"box-sizing: border-box !important;\">\r\n                                            <td style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; width: 4rem; padding-right: .5rem; padding-top: .5rem; padding-bottom: .5rem; vertical-align: top; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\" align=\"left\" valign=\"top\">\r\n                                                <img alt=\"\" src=\"{{imageUrl}}\" style=\"vertical-align: top; outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; max-width: 100% !important;\" />\r\n                                            </td>\r\n                                            <td style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; padding-top: .5rem; padding-bottom: .5rem; vertical-align: top !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\"\r\n                                                align=\"left\" valign=\"top\">\r\n                                                <div class=\"mv1-ns\" style=\"box-sizing: border-box; line-height: 1.25; !important;\">\r\n                                                    {{name}}\r\n                                                </div>\r\n                                                <div style=\"box-sizing: border-box; font-size: 14px; !important;\">\r\n                                                <b>{{quantity}}</b> quantity\r\n                                                </div>\r\n                                                <div style=\"box-sizing: border-box; font-size: 14px; !important;\">\r\n                                                ALU Code: {{refId}}\r\n                                                </div>\r\n                                                <div style=\"box-sizing: border-box; !important;\">\r\n                                                    \r\n                                                    {{#if sellingPrice}}\r\n                                                    Price: <b>$ {{formatUSDCurrency sellingPrice}}</b>\r\n                                                    {{else}}\r\n                                                    Free\r\n                                                    {{/if}}\r\n                                                </div>\r\n                                                <div style=\"box-sizing: border-box; !important;\">\r\n                                                    Tax: $ {{formatUSDCurrency (sumTaxesSku priceTags)}}\r\n                                                </div>\r\n                                            </td>\r\n                                            </tr>\r\n                                        </table>\r\n                                       {{/eq}}     \r\n                                    {{/each}}\r\n                            {{/eq}}\r\n                            {{/each}}\r\n                        </td>\r\n                    </tr>    \r\n                            \r\n                    <tr style=\"box-sizing: border-box !important;\">\r\n                        <td class=\"ph4-ns\"\r\n                            style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; border-top-style: solid; border-top-width: 1px; border-color: #eee; padding-top: 1.5rem; padding-bottom: 1rem !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\"\r\n                            align=\"left\">\r\n                            <span>Please sign here:</span>\r\n                            <div\r\n                            style=\"font-size: 10px;width:100%;left:50%;height:20px;line-height:20px;border-top:2px solid #D4D4D4;text-align:center;color:#333333\">\r\n                            Customer Signature\r\n                            </div>\r\n                        </td>\r\n                    </tr>\r\n\r\n                    <tr style=\"box-sizing: border-box !important;\">\r\n                        <td class=\"ph4-ns\"\r\n                            style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; border-top-style: solid; border-top-width: 1px; border-color: #eee; padding-top: 1.5rem; padding-bottom: 1rem !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\"\r\n                            align=\"left\">\r\n                            <span>Employee code:</span>\r\n                            <div\r\n                            style=\"font-size: 10px;width:100%;left:50%;height:20px;line-height:20px;border-top:2px solid #D4D4D4;text-align:center;color:#333333\">\r\n                            RPro Number\r\n                            </div>\r\n                        </td>\r\n                    </tr>\r\n\r\n                    <tr style=\"box-sizing: border-box !important;\">\r\n                        <td class=\"ph4-ns\"\r\n                            style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; border-top-style: solid; border-top-width: 1px; border-color: #eee; padding-top: 1rem; padding-bottom: 1rem !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\"\r\n                            align=\"left\">\r\n                            <p style=\"box-sizing: border-box !important;\">\r\n                            <a href=\"{{curbsidePickup.actionLink}}/\"> 1. Click here to notify the customer that the order is ready for pickup.</a>\r\n                            </p>\r\n                        </td>\r\n                    </tr>\r\n                    \r\n                    <tr style=\"box-sizing: border-box !important;\">\r\n                        <td class=\"ph4-ns\"\r\n                            style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; border-top-style: solid; border-top-width: 1px; border-color: #eee; padding-top: 1rem; padding-bottom: 1rem !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\"\r\n                            align=\"left\">\r\n                            <p style=\"box-sizing: border-box !important;\">\r\n                            <a href=\"{{curbsidePickup.cancelLink}}/\"> Cancel the Order.</a>\r\n                            </p>\r\n                        </td>\r\n                    </tr>\r\n\r\n                    <tr style=\"box-sizing: border-box !important;\">\r\n                        <td class=\"ph4-ns\"\r\n                            style=\"font-size: 14px; line-height: 20px; box-sizing: border-box; border-collapse: collapse; text-align: left; border-top-style: solid; border-top-width: 1px; border-color: #eee; padding-top: 1rem; padding-bottom: 1rem !important; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;\"\r\n                            align=\"left\">\r\n                            <p style=\"box-sizing: border-box !important;\">\r\n                                Regards,<br />Team {{_accountInfo.CompanyName}}.\r\n                            </p>\r\n                            <p>\r\n                                <span>Email Us:\r\n                                    <a class=\"\" href=\"mailto: support@storedomain.com\"\r\n                                        target=\"_blank\">support@storedomain.com</a>\r\n                                </span>\r\n                                <br />\r\n                                <span>Toll Free:\r\n                                    <a class=\"\" href=\"tel: +11111111111\">1-111-111-1111</a>\r\n                                </span>\r\n                                <br />\r\n                                <span>International:\r\n                                    <a class=\"\" href=\"tel: +11111111111\">1-111-111-1111</a>\r\n                                </span>\r\n                            </p>\r\n                        </td>\r\n                    </tr>\r\n                </table>\r\n            </td>\r\n        </tr>\r\n    </table>\r\n</body>\r\n\r\n</html>",
